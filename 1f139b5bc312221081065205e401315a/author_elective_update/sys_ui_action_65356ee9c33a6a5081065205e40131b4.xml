        <name>Reschedule</name>
        <onclick/>
        <order>100</order>
        <script><![CDATA[(function executeAction(current, gForm, gUser, gSNC) {
    
    var newDate = current.u_appointment_date;
    
    if (!newDate) {
        gs.addErrorMessage("Please enter a new appointment date before rescheduling.");
        return;
    }

    
    current.update();

    
    var patientGR = new GlideRecord('u_patient');
    if (patientGR.get(current.patient)) {
        gs.eventQueue('appointment.rescheduled', current, patientGR.sys_id, gUser.getID());
        gs.addInfoMessage("Patient has been notified about the rescheduled appointment.");
    } else {
        gs.addErrorMessage("Patient record not found.");
    }
})(current, gForm, gUser, gSNC);]]></script>
