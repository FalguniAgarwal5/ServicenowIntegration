        <script><![CDATA[var PatientUtils2 = Class.create();
PatientUtils2.prototype = Object.extendsObject(global.AbstractAjaxProcessor, {
    details: function() {
        var patientId = this.getParameter('sysparm_patient_id');
        var patientGR = new GlideRecord("u_patient");
        var details = {};

        if (patientGR.get(patientId)) {
            details.u_fn = patientGR.u_first_name.toString();
            details.id = patientGR.u_number.toString(); 

        return JSON.stringify(details);
	}},

	type: 'PatientUtils2'
});
]]></script>
