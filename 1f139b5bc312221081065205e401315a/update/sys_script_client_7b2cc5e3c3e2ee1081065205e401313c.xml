        <condition/>
        <description>Show a welcome message when form loads</description>
        <field/>
        <global>true</global>
        <isolate_script>true</isolate_script>
        <messages/>
        <name>Welcome Message On Patient Form</name>
        <order/>
        <script><![CDATA[function onLoad() {
    
    if (g_form.isNewRecord()) {
        g_form.addInfoMessage(" Welcome! You are creating a new patient record.");
    } else {
        var userName = g_form.getValue('u_first_name');
        g_form.addInfoMessage(" Welcome, " + userName + "! You are viewing an existing patient record.");
    }
}]]></script>
