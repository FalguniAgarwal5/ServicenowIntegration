        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading) {
    if (isLoading || newValue == '') return;

    var ga = new GlideAjax('DoctorUtils');
    ga.addParam('sysparm_name', 'getDoctorsByDepartment');
    ga.addParam('sysparm_department', newValue);
    ga.getXMLAnswer(function(response) {
        var doctors = JSON.parse(response);
        g_form.clearOptions('Doctor');
        g_form.addOption('Doctor', '', '-- Select Doctor --');
        doctors.forEach(function(doc) {
            g_form.addOption('Doctor', doc.sys_id, doc.name);
        });
    });
}]]></script>
