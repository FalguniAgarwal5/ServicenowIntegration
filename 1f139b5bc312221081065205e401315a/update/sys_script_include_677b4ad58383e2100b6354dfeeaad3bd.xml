        <script><![CDATA[var InsuranceUtils2 = Class.create();
InsuranceUtils2.prototype = Object.extendsObject(global.AbstractAjaxProcessor, {


    getInsuranceDetails: function() {
        var policyNum = this.getParameter('sysparm_policy_number');
        
        var gr = new GlideRecord('u_insurance'); 
        gr.addQuery('u_policy_number', policyNum); 
        gr.query();

        if (gr.next()) {
            return 'Provider: ' + gr.getValue('u_provider') + 
                   ' | Coverage: ' + gr.getValue('u_coverage_details');
        }

        return 'No insurance record found for this policy number.';
    }

});
    type: 'InsuranceUtils2'
]]></script>
