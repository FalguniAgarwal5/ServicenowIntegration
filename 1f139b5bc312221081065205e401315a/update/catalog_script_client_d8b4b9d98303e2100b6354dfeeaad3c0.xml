    var ga = new GlideAjax('Labutils');
    ga.addParam('sysparm_name', 'getAvailableSlots');
    ga.addParam('sysparm_date', newValue);
    ga.getXMLAnswer(function(response) {
        if (response === 'No available slots') {
            g_form.showFieldMsg('u_scheduled_date', response, 'error');
        } else {
            g_form.showFieldMsg('u_scheduled_date', 'Available Slots: ' + response, 'info');
        }
    });
