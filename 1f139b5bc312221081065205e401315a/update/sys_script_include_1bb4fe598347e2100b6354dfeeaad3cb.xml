        <script><![CDATA[var DoctorUtils = Class.create();
DoctorUtils.prototype = Object.extendsObject(global.AbstractAjaxProcessor, {
	
    
    getDoctorsByDepartment: function() {
        var dept = this.getParameter('sysparm_department');
        var doctorList = [];

        var gr = new GlideRecord('u_doctor'); 
        gr.addQuery('u_specialization', dept);
        gr.query();

        while (gr.next()) {
            doctorList.push({
                sys_id: gr.getValue('sys_id'),
                name: gr.getValue('u_first_name')
            });
        }
        return JSON.stringify(doctorList);
    }
});

    type: 'DoctorUtils'
]]></script>
